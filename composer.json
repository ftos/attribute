{
    "name": "ftos/attribute",
    "type": "library",
    "description": "Simple PHP attribute/value component library.",
    "keywords": ["attribute", "value", "eav"],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "doctrine/collections": "1.2.*@dev",
        "ftos/timestamp": "0.1.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*@dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Ftos\\Component\\Attribute\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ftos\\Component\\Attribute\\Tests\\": "Tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}